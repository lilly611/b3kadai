    "import numpy as np\n",
    "\n",
    "def calc_partial_sum(n):\n",
    "    s=0\n",
    "    for i in range(n):\n",
    "        s+=1/(i+1)**2\n",
    "    return s\n",
    "\n",
    "#Nを1000として計算してみる\n",
    "calc_partial_sum(1000)"
   ]
